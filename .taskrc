# [Created by task 2.5.0 1/6/2016 09:51:29]
# Taskwarrior program configuration file.
# For more documentation, see http://taskwarrior.org or try 'man task', 'man task-color',
# 'man task-sync' or 'man taskrc'

# Here is an example of entries that use the default, override and blank values
#   variable=foo   -- By specifying a value, this overrides the default
#   variable=      -- By specifying no value, this means no default
#   #variable=foo  -- By commenting out the line, or deleting it, this uses the default

# Use the command 'task show' to see all defaults and overrides

# Files
data.location=~/drive/.task

# Color theme
include /usr/local/Cellar/task/2.5.0_1/share/doc/task/rc/solarized-dark-256.theme
color.tagged=

confirmation=no
nag=

# Reporting aliases
alias.in=list +PENDING +in
alias.next=list +PENDING +next
alias.wait=list +PENDING +wait
alias.maybe=list +PENDING +maybe
alias.pend=list +PENDING

# Sorting
report.next.sort=project+,tags+,due+,entry+

# Other aliases
alias.rm=delete
alias.n=modify -in +next
alias.p=modify +palantir
alias.as=modify +aslan
alias.pi=modify +pilot
alias.np=modify -in +next +palantir
alias.nas=modify -in +next +aslan
alias.npi=modify -in +next +pilot
alias.m=modify -in +maybe
alias.wa=modify -in -next +wait

context.work=+palantir or +aslan or +pilot or +in
context.dcn=-palantir -aslan -pilot

# Priority
urgency.active.coefficient=0
urgency.age.coefficient=0
urgency.age.max=0
urgency.annotations.coefficient=0
urgency.blocked.coefficient=0
urgency.blocking.coefficient=0
urgency.due.coefficient=0
urgency.inherit=0
urgency.project.coefficient=0
urgency.scheduled.coefficient=0
urgency.tags.coefficient=0
urgency.uda.priority.H.coefficient=0
urgency.uda.priority.L.coefficient=0
urgency.uda.priority.M.coefficient=0
urgency.user.tag.next.coefficient=0
urgency.waiting.coefficient=0
